{
    "name": "carriongrow/formula_parser",
    "description": "Parses formulas, with the possibility of a deferred solution with substitution of user variables",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anatoliy Snegovskiy",
            "email": "CarrionGrow@Gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.5",
        "squizlabs/php_codesniffer": "^3",
        "vimeo/psalm": "^5"
    },
    "autoload": {
        "psr-4": {
            "CarrionGrow\\FormulaParser\\": "src/"
        }
    },
    "autoload-dev": {
        "CarrionGrow\\FormulaParser\\Test\\": "test/"
    },
    "minimum-stability": "dev"
